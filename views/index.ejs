                    <td class="resistance-unknown">FUS</td>
                    <td class="resistance-fail">MUP</td>
                    <td class="resistance-unknown">RIF</td>
                    <td class="resistance-unknown">TRI</td>
                  </tr>
                </tbody>
              </table>
            </div><!-- /.assembly-resistance-profile-container -->

